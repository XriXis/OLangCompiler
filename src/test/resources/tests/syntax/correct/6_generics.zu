class PrintValue[T] is
  this()

  method printSum(a: T, b: T) is
    Console().println(Integer(a.Plus(b)))
  end
end


class Main is
  this() is
    var printValueInt : PrintValue[Integer]
    printValueInt.printSum(2, 3)

    var printValueReal : PrintValue[Real]
    printValueReal.printSum(3.5, 2.5)
  end
end
