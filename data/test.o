class Main is
this() is
    var dog : Dog()
    var cat : Cat()
    this.saySpeak(dog)
    this.saySpeak(cat)
  end

  method saySpeak(animal: Animal) is
    var num : Integer
    num := animal.speak()
    Console().println(num)
  end
end

class Animal is
  this()

  method speak : Integer is
    return Integer(10)
  end

  method eat : Integer is
    return Integer(10)
  end

end


class Dog extends Animal is
  this()

  method speak : Integer is
    return Integer(5)
  end
end

class Cat extends Animal is
  this()

  method speak : Integer is
    return Integer(2)
  end
end
