class MyClass is
    var a : Integer(10)
    var b : Integer(20)

    this() is

    end

    this(c:Integer) is
        a := 10
        b := 20
        a := c
    end

    method add() : Integer is
        return a.Plus(b)
    end
end

class Main is
    this() is
        var obj : MyClass
        obj := MyClass()
        var result : Integer
        result := obj.add()
        Console().print(result.Get())
    end
end
