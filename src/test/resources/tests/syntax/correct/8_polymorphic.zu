class Main is
  this() is
    var dog : Dog()
    var cat : Cat()
    this.saySpeak(dog)
    this.saySpeak(cat)
  end

  method saySpeak(animal: Animal) is
    var num : Integer
    num := animal.speak()
    Console().println(num)
  end
end

class Animal is
  this()
  method speak : Integer => 10
end

class Dog extends Animal is
  this()
  method speak : Integer => 5
end

class Cat extends Animal is
  this()
  method speak : Integer => 6
end